{
  "name": "nano-banana-ai-photo-editor",
  "description": "Edit any photo with simple words using our advanced AI. Nano Banana delivers consistent results, superior to Flux Kontext.",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "npm run cf-typegen",
    "preview": "pnpm run build && vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "db:generate": "drizzle-kit generate --config app/.server/drizzle/config.ts",
    "db:migrate": "drizzle-kit migrate --config app/.server/drizzle/config.ts",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "monitor": "node scripts/quick-monitor.mjs",
    "monitor:detailed": "npx tsx scripts/monitor-image-generation.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@fontsource/libre-baskerville": "^5.2.6",
    "@markdoc/markdoc": "^0.5.2",
    "@playwright/test": "^1.55.0",
    "@react-oauth/google": "^0.12.2",
    "@react-router/cloudflare": "^7.5.3",
    "@react-router/fs-routes": "^7.5.3",
    "ai": "^4.3.16",
    "better-sqlite3": "^12.2.0",
    "chalk": "^5.3.0",
    "clsx": "^2.1.1",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.41.0",
    "glob": "^11.0.3",
    "google-auth-library": "^9.15.1",
    "isbot": "^5.1.27",
    "jszip": "^3.10.1",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.513.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-compare-slider": "^3.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "zod": "^3.25.56",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.12",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^5.0.2",
    "daisyui": "^5.0.43",
    "drizzle-kit": "^0.31.1",
    "jsdom": "^26.1.0",
    "miniflare": "^4.20250823.1",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.19.1"
  }
}
